{
    "name": "dominus77/yii2-sweetalert2-widget",
    "description": "Renders a SweetAlert2 widget for Yii2.",
    "type": "yii2-extension",
    "keywords": ["dominus77","yii2","extension","widget","sweetalert2"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexey Schevchenko",
            "email": "ivanovosity@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0",
        "npm-asset/sweetalert2": "^11.4",
        "npm-asset/sweetalert2--themes": "^5.0",
        "npm-asset/animate.css": "^4.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^6.5"
    },
    "repositories":[
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "dominus77\\sweetalert2\\": "src"
        }
    }
}
